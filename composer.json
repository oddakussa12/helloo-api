{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "aws/aws-sdk-php": "^3.147",
        "baopham/dynamodb": "^6.0",
        "barryvdh/laravel-cors": "^0.11.3",
        "brozot/laravel-fcm": "^1.3",
        "dimsav/laravel-translatable": "8.1",
        "dingo/api": "2.2.3",
        "fenos/notifynder": "^4.3",
        "fideloper/proxy": "~3.3",
        "geoip2/geoip2": "~2.1",
        "giggsey/libphonenumber-for-php": "^8.12",
        "google/cloud": "^0.129.0",
        "jenssegers/agent": "^2.6",
        "jpush/jpush": "^3.6",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "~3.2",
        "laravel/tinker": "~1.0",
        "mcamara/laravel-localization": "^1.3",
        "mews/purifier": "^3.1",
        "overtrue/laravel-filesystem-qiniu": "^1.0",
        "overtrue/laravel-follow": "^1.1",
        "overtrue/laravel-lang": "~3.0",
        "overtrue/laravel-like": "^1.0",
        "predis/predis": "^1.1",
        "qiniu/php-sdk": "^7.2",
        "rongcloud/php-sdk": "^3.0",
        "spatie/laravel-tags": "2.0.0",
        "tamayo/laravel-scout-elastic": "^5.0",
        "toplan/laravel-sms": "~2.6",
        "torann/geoip": "^1.0",
        "tymon/jwt-auth": "1.0.0-rc.4.1",
        "weboap/visitor": "^2.0"
    },
    "require-dev": {
        "beyondcode/laravel-query-detector": "^1.0",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files":["app/helps.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
